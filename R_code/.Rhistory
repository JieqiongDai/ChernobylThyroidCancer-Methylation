library(minfi)
load("../data/inital_data.Rdata")
dmp1 <- dmpFinder(beta, pheno = pheno $ Case.Control  , type = "categorical")
sig_dmp1 <- subset(dmp1,qval<=0.01)
nrow(sig_dmp1)
annot <- read.csv("../all_sample/data/MethylationEPIC_v-1-0_B2_anno.csv",header=T)
head(annot)
annot_gene <- annot[,c(1,15:20,25,27:33,44)]
library(data.table)
sig_dmp_data1 <- setDT(sig_dmp1, keep.rownames = TRUE)[]
head(sig_dmp_data1)
sig_dmp_anno1 <- merge(sig_dmp_data1,annot_gene,by.x="rn",by.y="IlmnID")
head(sig_dmp_anno1)
nrow(sig_dmp_anno1)
